@model IEnumerable<LeetSpeak1.Models.L33tSp34k>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Add New Data</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Translated")
        </th>
        <th>
            @Html.DisplayName("Text")
        </th>
        <th>
            @Html.DisplayName("Translation")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Translated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Translation)
        </td>
       
    </tr>
}
</table>
